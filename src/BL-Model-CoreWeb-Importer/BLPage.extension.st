Extension { #name : #BLPage }

{ #category : #'*BL-Model-CoreWeb-Importer' }
BLPage >> addWidget: aWidget [
	self businessPage
		ifEmpty: [ self businessPage
				add:
					(BLBusinessPage new
						name: 'autoGenerated';
						addWidget: aWidget) ]
		ifNotEmpty: [ (self businessPage at: 1) addWidget: aWidget ]
]
